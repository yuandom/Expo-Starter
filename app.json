{
  "expo": {
    "sdkVersion": "53.0.0",
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/app/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./src/app/assets/notification-sound.wav"
          ]
        }
      ],
      "expo-sqlite"
    ],
    "name": "expotemplate",
    "slug": "expotemplate"
  }
}
